[
    {
        "comment": "账号密码登录",
        "request": {
            "method": "POST",
            "url": "https://ai1foo.com/api/v1/user/login",
            "headers": [
                {
                    "name": "Host",
                    "value": "ai1foo.com"
                },
                {
                    "name": "Connection",
                    "value": "keep-alive"
                },
                {
                    "name": "sec-ch-ua",
                    "value": "\"Chromium\";v=\"122\", \"Not(A:Brand\";v=\"24\", \"Google Chrome\";v=\"122\""
                },
                {
                    "name": "Content-Type",
                    "value": "application/json"
                },
                {
                    "name": "sec-ch-ua-mobile",
                    "value": "?0"
                },
                {
                    "name": "User-Agent",
                    "value": "Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/122.0.6261.95 Safari/537.36"
                },
                {
                    "name": "app-name",
                    "value": "bianjie"
                },
                {
                    "name": "sec-ch-ua-platform",
                    "value": "\"Windows\""
                },
                {
                    "name": "Accept",
                    "value": "*/*"
                },
                {
                    "name": "Origin",
                    "value": "https://ai1foo.com"
                },
                {
                    "name": "Sec-Fetch-Site",
                    "value": "same-origin"
                },
                {
                    "name": "Sec-Fetch-Mode",
                    "value": "cors"
                },
                {
                    "name": "Sec-Fetch-Dest",
                    "value": "empty"
                },
                {
                    "name": "Referer",
                    "value": "https://ai1foo.com/login"
                },
                {
                    "name": "Accept-Encoding",
                    "value": "gzip, deflate, br"
                },
                {
                    "name": "Accept-Language",
                    "value": "zh-CN,zh;q=0.9"
                }
            ],
            "cookies": [],
            "data": "{\"user\":\"{{username}}\",\"pass\":\"{{password}}\"}",
            "mimeType": "application/json"
        },
        "rule": {
            "success_asserts": [
                {
                    "re": "200",
                    "from": "status"
                },
                {
                    "re": "ok",
                    "from": "content"
                }
            ],
            "failed_asserts": [
                {
                    "re": "用户不存在或密码错误",
                    "from": "content"
                }
            ],
            "extract_variables": [
                {
                    "name": "yhm",
                    "re": "\"user\":\"(.*?)\",",
                    "from": "content"
                },
                {
                    "name": "token",
                    "re": "\"token\":\"(.*?)\",",
                    "from": "content"
                },
                {
                    "name": "access_token",
                    "re": "\"access_token\":\"(.*?)\"",
                    "from": "content"
                },
                {
                    "name": "appid",
                    "re": "\"appid\":(\\d+)",
                    "from": "content"
                },
                {
                    "name": "id",
                    "re": "\"id\":(\\d+)",
                    "from": "content"
                }
            ]
        }
    },
