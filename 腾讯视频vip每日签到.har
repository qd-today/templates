    },
    {
        "comment": "cookie_字符串替换",
        "request": {
            "method": "POST",
            "url": "api://util/string/replace",
            "headers": [],
            "cookies": [],
            "data": "r=json&p=vusession=[^;]*&s={{txspCookie}}&t=vusession={{vusession_new}}"
        },
        "rule": {
            "success_asserts": [
                {
                    "re": "200",
                    "from": "status"
                },
                {
                    "re": "\"状态\": \"OK\"",
                    "from": "content"
                }
            ],
            "failed_asserts": [],
            "extract_variables": [
                {
                    "name": "cookie",
                    "re": "\"处理后字符串\": \"(.*)\"",
                    "from": "content"
                }
            ]
        }
    },
