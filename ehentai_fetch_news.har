[
    {
        "comment": "Test login",
        "request": {
            "method": "GET",
            "url": "https://forums.e-hentai.org/",
            "headers": [
                {
                    "name": "accept\n",
                    "value": "text/html,application/xhtml+xml,application/xml;q=0.9,image/jxl,image/avif,image/webp,image/apng,*/*;q=0.8,application/signed-exchange;v=b3;q=0.7"
                },
                {
                    "name": "accept-encoding",
                    "value": "gzip, deflate, br, zstd"
                },
                {
                    "name": "dnt",
                    "value": "1"
                },
                {
                    "name": "priority",
                    "value": "u=0, i"
                },
                {
                    "name": "sec-ch-ua",
                    "value": "\"Not?A_Brand\";v=\"99\", \"Chromium\";v=\"130\""
                },
                {
                    "name": "sec-ch-ua-mobile",
                    "value": "?0"
                },
                {
                    "name": "sec-ch-ua-platform",
                    "value": "\"Linux\""
                },
                {
                    "name": "sec-fetch-dest",
                    "value": "document"
                },
                {
                    "name": "sec-fetch-mode",
                    "value": "navigate"
                },
                {
                    "name": "sec-fetch-site\n",
                    "value": "none"
                },
                {
                    "name": "sec-fetch-user",
                    "value": "?1"
                },
                {
                    "name": "sec-gpc",
                    "value": "1"
                },
                {
                    "name": "upgrade-insecure-requests",
                    "value": "1"
                },
                {
                    "name": "user-agent",
                    "value": "Mozilla/5.0 (X11; Linux x86_64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/130.0.0.0 Safari/537.36"
                },
                {
                    "name": "cookie",
                    "value": "{{cookie}}"
                }
            ],
            "cookies": [],
            "data": ""
        },
        "rule": {
            "success_asserts": [
                {
                    "re": "200",
                    "from": "status"
                }
            ],
            "failed_asserts": [
                {
                    "re": "Welcome Guest",
                    "from": "content"
                }
            ],
            "extract_variables": [
                {
                    "name": "user_id",
                    "re": "<a href=\"https:\\/\\/forums\\.e-hentai\\.org\\/index\\.php\\?.{0,}?showuser=(\\d+)\">.+?<\\/a>",
                    "from": "content"
                }
            ]
        }
    },
