    },
    {
        "comment": "Remove HTML tags",
        "request": {
            "method": "POST",
            "url": "api://util/unicode",
            "headers": [],
            "cookies": [],
            "data": "html_unescape=false&content={{event_content|striptags}}"
        },
        "rule": {
            "success_asserts": [
                {
                    "re": "200",
                    "from": "status"
                }
            ],
            "failed_asserts": [
                {
                    "re": "You have encountered a monster!",
                    "from": "content"
                }
            ],
            "extract_variables": [
                {
                    "name": "event_result",
                    "re": "\"转换后\": \"(.+)\"",
                    "from": "content"
                }
            ]
        }
    },
    {
        "comment": "Print log",
        "request": {
            "method": "POST",
            "url": "api://util/unicode",
            "headers": [],
            "cookies": [],
            "data": "html_unescape=false&content=User ID: {{user_id}} \\r\\n Result: {{event_result}}"
        },
        "rule": {
            "success_asserts": [
                {
                    "re": "200",
                    "from": "status"
                }
            ],
            "failed_asserts": [],
            "extract_variables": [
                {
                    "name": "__log__",
                    "re": "\"转换后\": \"(.+)\"",
                    "from": "content"
                }
            ]
        }
    }
]