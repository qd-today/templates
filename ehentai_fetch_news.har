                    "value": "u=0, i"
                },
                {
                    "name": "sec-ch-ua",
                    "value": "\"Not?A_Brand\";v=\"99\", \"Chromium\";v=\"130\""
                },
                {
                    "name": "sec-ch-ua-mobile",
                    "value": "?0"
                },
                {
