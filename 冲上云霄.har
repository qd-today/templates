[
    {
        "comment": "登录",
        "request": {
            "method": "POST",
            "url": "https://999865.xyz/api/user/login",
            "headers": [
                {
                    "name": "Host",
                    "value": "999865.xyz"
                },
                {
                    "name": "Connection",
                    "value": "keep-alive"
                },
                {
                    "name": "sec-ch-ua",
                    "value": "\"Chromium\";v=\"122\", \"Not(A:Brand\";v=\"24\", \"Google Chrome\";v=\"122\""
                },
                {
                    "name": "Accept",
                    "value": "application/json, text/plain, */*"
                },
                {
                    "name": "Content-Type",
                    "value": "application/json"
                },
                {
                    "name": "sec-ch-ua-mobile",
                    "value": "?0"
                },
                {
                    "name": "User-Agent",
                    "value": "Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/122.0.6261.95 Safari/537.36"
                },
                {
                    "name": "sec-ch-ua-platform",
                    "value": "\"Windows\""
                },
                {
                    "name": "Origin",
                    "value": "https://999865.xyz"
                },
                {
                    "name": "Sec-Fetch-Site",
                    "value": "same-origin"
                },
                {
                    "name": "Sec-Fetch-Mode",
                    "value": "cors"
                },
                {
                    "name": "Sec-Fetch-Dest",
                    "value": "empty"
                },
                {
                    "name": "Referer",
                    "value": "https://999865.xyz/login"
                },
                {
                    "name": "Accept-Encoding",
                    "value": "gzip, deflate, br"
                },
                {
                    "name": "Accept-Language",
                    "value": "zh-CN,zh;q=0.9"
                }
            ],
            "cookies": [],
            "data": "{\"username\":\"{{username}}\",\"password\":\"{{password}}\"}",
            "mimeType": "application/json"
        },
        "rule": {
            "success_asserts": [
                {
                    "re": "200",
                    "from": "status"
                },
                {
                    "re": "登录成功",
                    "from": "content"
                }
            ],
            "failed_asserts": [
                {
                    "re": "信息不匹配",
                    "from": "content"
                }
            ],
            "extract_variables": [
                {
                    "name": "token",
                    "re": "\"data\":\"(.+)\",",
                    "from": "content"
                }
            ]
        }
    },
