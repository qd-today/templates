[
    {
        "comment": "登陆",
        "request": {
            "method": "POST",
            "url": "https://api.mall.leyifan.com/api/front/login/leyifan",
            "headers": [
                {
                    "name": "system",
                    "value": "ios"
                },
                {
                    "name": "App-Version",
                    "value": "{{version}}"
                },
                {
                    "name": "Accept",
                    "value": "*/*"
                },
                {
                    "name": "Accept-Language",
                    "value": "en-GB,en-US;q=0.9,en;q=0.8"
                },
                {
                    "name": "Accept-Encoding",
                    "value": "gzip, deflate, br"
                },
                {
                    "name": "platform",
                    "value": "app"
                },
                {
                    "name": "Content-Type",
                    "value": "application/json"
                },
                {
                    "name": "User-Agent",
                    "value": "Mozilla/5.0 (iPhone; CPU iPhone OS 18_7 like Mac OS X) AppleWebKit/605.1.15 (KHTML, like Gecko) Mobile/15E148 Html5Plus/1.0 (Immersed/44) uni-app"
                },
                {
                    "name": "lang",
                    "value": "chs"
                },
                {
                    "name": "Connection",
                    "value": "keep-alive"
                },
                {
                    "name": "Appplatform",
                    "value": "ios_other"
                },
                {
                    "name": "Origin-channel",
                    "value": "ios_gat"
                },
                {
                    "name": "UseDomain",
                    "value": "leyifan"
                }
            ],
            "cookies": [],
            "data": "{\"password\":\"{{password}}\",\"account\":\"{{email}}\",\"version\":\"V2\"}",
            "mimeType": "application/json"
        },
        "rule": {
            "success_asserts": [
                {
                    "re": "200",
                    "from": "status"
                }
            ],
            "failed_asserts": [],
            "extract_variables": [
                {
                    "name": "token",
                    "re": "\"token\"\\s*:\\s*\"([^\"]+)\"",
                    "from": "content"
                }
            ]
        }
    },
