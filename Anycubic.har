    {
        "comment": "正则提取",
        "request": {
            "method": "POST",
            "url": "api://util/regex",
            "headers": [],
            "cookies": [],
            "data": "p=mo_access_token=(.*?);&data={{cookie}}"
        },
        "rule": {
            "success_asserts": [
                {
                    "re": "200",
                    "from": "status"
                },
                {
                    "re": "\"状态\": \"OK\"",
                    "from": "content"
                }
            ],
            "failed_asserts": [],
            "extract_variables": [
                {
                    "name": "token",
                    "re": "\"1\": \"(.*)\"",
                    "from": "content"
                }
            ]
        }
    },
