            "url": "api://util/dddd/ocr",
            "headers": [
                {
                    "name": "Content-Type",
                    "value": "application/json"
                }
            ],
            "cookies": [],
            "data": "{\"img\":\"{{base64IMG}}\",\"imgurl\":\"\",\"old\":\"False\",\"extra_onnx_name\":\"\"}"
        },
        "rule": {
            "success_asserts": [
                {
                    "re": "200",
                    "from": "status"
                },
                {
                    "re": "\"状态\": \"OK\"",
                    "from": "content"
                }
            ],
            "failed_asserts": [],
            "extract_variables": [
                {
                    "name": "yzm",
                    "re": "\"Result\": \"(.*)\"",
                    "from": "content"
                }
            ]
        }
    },
